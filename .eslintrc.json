{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
      "plugin:@typescript-eslint/recommended",
      "plugin:vue/vue3-essential",
      "standard"
  ],
  "parserOptions": {
      "ecmaVersion": 10,
      "parser": "@typescript-eslint/parser",
      "sourceType": "module"
  },
  "plugins": [
      "vue",
      "@typescript-eslint"
  ],
  "globals": {
    "API": true,
    "Info": true,
    "defineProps": true,
    "defineEmits": true
  },
  "rules": {
  }
}